<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.sheet_general
					</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
				 <settings.consentModel>
        				<TCEforms>
         					<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.consentModel</label>
         						<config>
								<type>radio</type>
								<items type="array">
									<numIndex index="0" type="array">
									        <numIndex index="0">Information Only</numIndex>
									        <numIndex index="1">information_only</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
									        <numIndex index="0">Implicit</numIndex>
									        <numIndex index="1">implicit</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
									        <numIndex index="0">Explicit</numIndex>
									        <numIndex index="1">explicit</numIndex>
									</numIndex>
								</items>
								</config>
        				</TCEforms>
       				</settings.consentModel>
					<settings.themeTitle>
						<TCEforms>
							<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_title</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</settings.themeTitle>
					<settings.introductory_text>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.introductory_text
							</label>
							<config>
								<type>text</type>
								<cols>30</cols>
								<rows>5</rows>
							</config>
						</TCEforms>
					</settings.introductory_text>
					<settings.additional_text>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.additional_text
							</label>
							<config>
								<type>text</type>
								<cols>30</cols>
								<rows>5</rows>
							</config>
						</TCEforms>
					</settings.additional_text>
					<settings.extra_options>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.extra_options
							</label>
							<config>
								<type>text</type>
								<cols>30</cols>
								<rows>5</rows>
							</config>
						</TCEforms>
					</settings.extra_options>
					<settings.countrySelection>
			        	<TCEforms>
			        		<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_country_enable</label>
					 			<onChange>reload</onChange>
			         				<config>
			              			<type>check</type>
			         				</config>
			        	</TCEforms>
			    	</settings.countrySelection>
   					<settings.countryField>
						<TCEforms>
							<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_country_list</label>
							<displayCond>FIELD:settings.countrySelection:=:1</displayCond>
								<config>
									<type>input</type></config>
						</TCEforms>
					</settings.countryField>
					<settings.anlayticsCode>
        				<TCEforms>
         					<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.anlyticsCode</label>
         						<config>
          							<type>input</type></config>
        				</TCEforms>
       				</settings.anlayticsCode>
				</el>
			</ROOT>
		</sDEF>
		<display>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_options</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
       				<settings.themeSelect>
						<TCEforms>
							<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.select_theme</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_default</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_grey</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_black</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
									  <numIndex index="0">LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_blue</numIndex>
										<numIndex index="1">3</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_custom</numIndex>
										<numIndex index="1">4</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</settings.themeSelect>
          			<settings.customThemeSelect>
						<TCEforms type="array">
							<config type="array">
								<type>input</type>
								<size>30</size>
								<max>256</max>
								<checkbox></checkbox>
								<eval>trim</eval>
								<wizards type="array">
									<_PADDING type="integer">2</_PADDING>
									<link type="array">
										<type>popup</type>
										<title>Link</title>
										<icon>link_popup.gif</icon>
										<module type="array">
                                            <name>wizard_element_browser</name>
                                            <urlParameters type="array">
												<mode>wizard</mode>
												<act>file</act>
                                            </urlParameters>
                                    	</module>
										<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
							<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.upload_custom_theme</label>
							<displayCond>FIELD:settings.themeSelect:=:4</displayCond>
						</TCEforms>
					</settings.customThemeSelect>
       				<settings.iconType>
						<TCEforms>
							<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.icon_type
							</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.diamond_view</numIndex>
										<numIndex index="0">Diamond</numIndex>
            							<numIndex index="1">diamon</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.triangle_view</numIndex>
										<numIndex index="0">Triangle</numIndex>
            							<numIndex index="1">triangle</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</settings.iconType>
					<settings.iconPosition>
						<TCEforms>
							<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.icon_position
							</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.icon_position_right</numIndex>
										<numIndex index="0">Right</numIndex>
            							<numIndex index="1">right</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.icon_position_left</numIndex>
										<numIndex index="0">Left</numIndex>
            							<numIndex index="1">left</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</settings.iconPosition>
					<settings.enableSession>
			        	<TCEforms>
			        		<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.theme_enable_session</label>
							<config>
								<type>check</type>
							</config>
			        	</TCEforms>
			    	</settings.enableSession>
			    	<settings.implicit_showWidget>
			        	<TCEforms>
			        		<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.implicit_showWidget</label>
							<config>
								<type>check</type>
							</config>
			        	</TCEforms>
					</settings.implicit_showWidget>
					<settings.implicit_timeout>
			        	<TCEforms>
			        		<label>LLL:EXT:cookie_control/Resources/Private/Language/locallang_db.xlf:cookie_control.pi_flexform.implicit_timeout</label>
								<config>
									<type>input</type>
								</config>
			        	</TCEforms>
			    	</settings.implicit_timeout>
				</el>
			</ROOT>
		</display>
	</sheets>
</T3DataStructure>